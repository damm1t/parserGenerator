Start: add

NonTerms:
add
	: mul add_ 
;
add_
	: ADD mul add_ 
	| MINUS mul add_ 
	| EPS 
;
mul
	: term mul_ 
;
mul_
	: MUL term mul_ 
	| DIV term mul_ 
	| EPS 
;
term
	: NUM 
	| TERM0 add TERM1 
;

Terms:
DIV
	: /;
ADD
	: +;
TERM0
	: (;
MUL
	: *;
TERM1
	: );
NUM
	: /[1-9][0-9]*|0/;
EPS
	: EPS;
END
	: END;
MINUS
	: -;

First:
add : [TERM0, NUM]
mul : [TERM0, NUM]
mul_ : [DIV, MUL, EPS]
add_ : [ADD, EPS, MINUS]
term : [TERM0, NUM]

Follow:
add : [TERM1, END]
add_ : [TERM1, END]
mul : [ADD, TERM1, END, MINUS]
mul_ : [ADD, TERM1, END, MINUS]
term : [DIV, ADD, MUL, TERM1, END, MINUS]

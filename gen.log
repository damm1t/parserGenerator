Start: add

NonTerms:
add
	: mul add_ 
;
add_
	: ADD mul add_ 
	| MINUS mul add_ 
	| EPS 
;
mul
	: term mul_ 
;
mul_
	: MUL term mul_ 
	| DIV term mul_ 
	| EPS 
;
num1
	: NUM 
;
term
	: TERM0 num1 TERM1 num2 TERM2 
	| TERM3 add TERM4 
;
num2
	: NUM 
;

Terms:
DIV
	: /;
ADD
	: +;
TERM0
	: {;
MUL
	: *;
TERM1
	: ;;
TERM2
	: };
TERM3
	: (;
NUM
	: /[1-9][0-9]*|0/;
TERM4
	: );
EPS
	: EPS;
END
	: END;
MINUS
	: -;

First:
add : [TERM0, TERM3]
mul : [TERM0, TERM3]
mul_ : [DIV, MUL, EPS]
add_ : [ADD, EPS, MINUS]
num1 : [NUM]
term : [TERM0, TERM3]
